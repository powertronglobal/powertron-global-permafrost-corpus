  "title": "ASHRAE Climate Zone Reference",
  "version": "1.0.0",
  "last_updated": "2026-01-02",
  "purpose": "Maps geographic locations to ASHRAE climate zones for context",
  "note": "Climate zone does not affect kW/ton efficiency measurements (which are instantaneous), but provides context for annual energy savings estimates",
  "zone_definitions": {
    "1A": "Very Hot-Humid (cooling-dominated, >5000 CDD)",
    "2A": "Hot-Humid (cooling-dominated, 3500-5000 CDD)",
    "2B": "Hot-Dry (cooling-dominated, 3500-5000 CDD)",
    "3A": "Warm-Humid (mixed, 2500-3500 CDD)",
    "3B": "Warm-Dry (mixed, 2500-3500 CDD)",
    "4A": "Mixed-Humid (mixed, heating and cooling)",
    "5A": "Cool-Humid (heating-dominated, 3000-4000 HDD)",
    "6A": "Cold-Humid (heating-dominated, 4000-5000 HDD)"
  },
